<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="jadepc.breastradiographyapp.Achievement"
    android:background="@color/white">

    <!--
    <ImageView
        android:id="@+id/achievement"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"

        android:layout_marginLeft="3dp"
        android:contentDescription="achievement_picture"
        android:src="@drawable/achievements"


        -->

    <ImageView
        android:id="@+id/tenRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:src="@drawable/achievement_10" />

    <ImageView
        android:id="@+id/twentyRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tenRight"
        android:layout_margin="20dp"
        android:src="@drawable/achievement_10" />

    <ImageView
        android:id="@+id/fiftyRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/twentyRight"
        android:layout_margin="20dp"
        android:layout_marginLeft="140dp"
        android:src="@drawable/achievement_10" />

    <ImageView
        android:id="@+id/oneHundredRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tenRight"
        android:layout_margin="20dp"
        android:layout_marginLeft="140dp"
        android:src="@drawable/achievement_10" />

    <ImageView
        android:id="@+id/newHighScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/twentyRight"
        android:layout_toRightOf="@id/oneHundredRight"
        android:layout_margin="20dp"
        android:layout_marginLeft="140dp"
        android:src="@drawable/achievement_10" />

</RelativeLayout>
